{
  "name": "cisco-webex-sample",
  "version": "1.0.0",
  "description": "Sample app for a cisco webex pitch.",
  "main": "./src/index.js",
  "scripts": {
    "test": "Error no test specified",
    "build": "cross-env NODE_ENV=production webpack --env=production --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack serve --env=development --hot --inline --open --config webpack.config.js",
    "clean": "del -r node_modules",
    "reinstall": "npm run clean && npm install",
    "lint": "eslint src --ext .js,.tsx,.ts,.jsx --cache --max-warnings 0 && stylelint \"src/**/*.(css|scss)\"",
    "lint:fix": "eslint src --ext .js,.tsx,.ts,.jsx --cache --fix --max-warnings 0 && stylelint \"src/**/*.(css|scss)\"",
    "lint:style": "style \"src/**/*.(css)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenjiFischman/CiscoWebexSampleDemo.git"
  },
  "keywords": [
    "Demo",
    "video",
    "messaging"
  ],
  "author": "Benjamin Fischman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BenjiFischman/CiscoWebexSampleDemo/issues"
  },
  "homepage": "https://github.com/BenjiFischman/CiscoWebexSampleDemo#readme",
